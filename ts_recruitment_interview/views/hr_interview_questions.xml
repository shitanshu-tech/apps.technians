<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hr_interview_questions_view_form" model="ir.ui.view">
        <field name="name">hr.interview.questions.form</field>
        <field name="model">hr.interview.questions</field>
        <field name="arch" type="xml">
            <form string="Questions">
                <sheet>
                    <group>
                        <field name="interview_id" invisible="1"/>
                        <field name="name"/>
                        <field name="rating"/>
                        <field name="feedback"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
